    {% comment %} === æ ¸å¿ƒæ•°æ®æºé€»è¾‘é‡æ„ === {% endcomment %}
    {% assign use_blocks = false %}
    {% assign tarot_items = blank %}

    {% if section.settings.source_mode == 'specific_collection' %}
        {% comment %} æ¨¡å¼A: æŒ‡å®šç³»åˆ— {% endcomment %}
        {% assign target_collection = section.settings.source_collection %}
        {% if target_collection != blank %}
            {% assign tarot_items = target_collection.products | limit: section.settings.collection_limit %}
        {% else %}
            <p style="color:#666;text-align:center;width:100%;">[ç‚¼é‡‘æœ¯å£«æç¤º] è¯·åœ¨å³ä¾§é€‰æ‹©ä¸€ä¸ªäº§å“ç³»åˆ—ã€‚</p>
        {% endif %}

    {% elsif section.settings.source_mode == 'auto' %}
        {% comment %} æ¨¡å¼B: è‡ªåŠ¨æ¢æµ‹ {% endcomment %}
        {% if request.page_type == 'product' %}
            {% assign tarot_items = recommendations.products | default: collections.all.products | limit: 4 %} 
        {% elsif request.page_type == 'collection' %}
            {% assign tarot_items = collection.products | limit: section.settings.collection_limit %}
        {% else %}
            {% assign use_blocks = true %}
        {% endif %}

    {% else %}
        {% comment %} æ¨¡å¼C: æ‰‹åŠ¨å— {% endcomment %}
        {% assign use_blocks = true %}
    {% endif %}

    {% assign render_list = blank %}
    {% if use_blocks %} {% assign render_list = section.blocks %} {% else %} {% assign render_list = tarot_items %} {% endif %}

    {% for item in render_list %}
      {% if use_blocks %} {% assign p = item.settings.target_product %} {% else %} {% assign p = item %} {% endif %}
      
      {% if p != blank %}
        {% assign raw_meta_img = p.metafields.custom.tarot_front_image %}
        {% assign final_img_src = blank %}
        {% if raw_meta_img != blank %}
           {% assign meta_str = raw_meta_img | append: "" %}
           {% if meta_str contains 'http' %} {% assign final_img_src = meta_str %}
           {% else %} {% assign final_img_src = raw_meta_img | image_url: width: 600 %} {% endif %}
        {% else %}
           {% assign final_img_src = p.featured_image | image_url: width: 600 %}
        {% endif %}

        {% assign t_verdict = p.metafields.custom.tarot_verdict | newline_to_br | strip_newlines | replace: '<br />', '|||' | escape %}
        {% assign t_decree = p.metafields.custom.tarot_decree | default: "ENTER THE VOID" %}
        {% assign t_vendor = p.vendor | default: "UNK-000" %}

        {% comment %} é¢œè‰²å˜é‡è®¡ç®— {% endcomment %}
        {% assign source_color = section.settings.default_text_color %}
        {% if section.settings.use_soul_color_mode %}
              {% assign meta_color = p.metafields.custom.soul_color %}
              {% if meta_color != blank %}
                {% assign source_color = meta_color %}
              {% endif %}
        {% endif %}

        {% assign active_text_color = source_color %}
        {% assign active_border = source_color | color_modify: 'alpha', 0.5 %}
        {% assign active_light = source_color | color_modify: 'alpha', 0.8 %}
        
        {% assign salt = forloop.index | times: 19 %}
        {% assign seed = "now" | date: "%S" | plus: salt %}
        {% assign random_phase_1 = seed | modulo: 20 | times: -1.3 %}
        {% assign random_phase_2 = seed | times: 3 | modulo: 15 | times: -1.1 %}
        {% assign entry_delay = forloop.index0 | times: 0.1 %}

        <div class="tarot-scene" 
             data-verdict="{{ t_verdict }}"
             style="--active-text-color: {{ active_text_color }};
                    --active-border: {{ active_border }};
                    --active-light: {{ active_light }}; 
                    --entry-delay: {{ entry_delay }}s;
                    --random-phase-1: {{ random_phase_1 }}s;
                    --random-phase-2: {{ random_phase_2 }}s;">
          <div class="tarot-card">
            <div class="card-face card-face-front">
              {% if final_img_src != blank %}
                <img src="{{ final_img_src }}" alt="{{ p.title }}" loading="lazy">
              {% else %}
                 <div style="width:100%;height:100%;display:flex;align-items:center;justify-content:center;color:#555;background:#111;">VOID</div>
              {% endif %}
            </div>
            <div class="card-face card-face-back">
              <div class="energy-code">{{ t_vendor }}</div>
              <div class="oracle-text">...</div>
              <a href="{{ p.url }}" class="decree-btn">{{ t_decree }}</a>
            </div>
          </div>
        </div>
      {% endif %}
    {% endfor %}
  </div>
</div>

<script>
  (function() {
    function initTarotCards(scope) {
      const scenes = scope.querySelectorAll('.tarot-scene');
      scenes.forEach(scene => {
        if(scene.dataset.init === "true") return;
        scene.dataset.init = "true";
        const card = scene.querySelector('.tarot-card');
        const oracleBox = scene.querySelector('.oracle-text');
        const rawVerdict = scene.dataset.verdict;
        let fateDeck = [];
        if (rawVerdict) {
          try {
            const decodedVerdict = new DOMParser().parseFromString(rawVerdict, "text/html").documentElement.textContent;
            fateDeck = decodedVerdict.split('|||').filter(line => line.trim() !== '');
          } catch (e) { console.warn("Oracle decode failed", e); }
        }
        if (fateDeck.length === 0) fateDeck = ["The Void Stares Back.", "Silence is the Answer."];
        let isFlipped = false;
        scene.addEventListener('click', function(e) {
          if(e.target.closest('.decree-btn')) { e.stopPropagation(); return; }
          if (!isFlipped) {
            const randomLine = fateDeck[Math.floor(Math.random() * fateDeck.length)];
            oracleBox.innerHTML = randomLine;
            card.classList.add('is-flipped');
            setTimeout(() => { oracleBox.classList.add('visible'); }, 500); 
            isFlipped = true;
          } else {
            card.classList.remove('is-flipped');
            oracleBox.classList.remove('visible');
            isFlipped = false;
          }
        });
      });
    }
    document.addEventListener('DOMContentLoaded', () => initTarotCards(document));
    document.addEventListener('shopify:section:load', function(e) {
      if(e.target.querySelector('.tarot-hologram-wrapper')) initTarotCards(e.target);
    });
  })();
</script>

